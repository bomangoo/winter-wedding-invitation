{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:svelte3/recommended",
      "prettier"
    ],
    "plugins": [
      "svelte3"
    ],
    "overrides": [
      {
        "files": ["*.svelte"],
        "processor": "svelte3/svelte3"
      }
    ],
    "ignorePatterns": ["node_modules/"],
    "settings": {
      "svelte3/preprocess": true
    }
  }